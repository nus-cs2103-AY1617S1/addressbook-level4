<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.address.ui.TodoListPanel">
    <stylesheets>
        <URL value="@DarkTheme.css" />
        <URL value="@Extensions.css" />
    </stylesheets>
    <children>
      <TextArea editable="false" prefHeight="23.0" prefWidth="248.0" text="Todos">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </TextArea>
        <ListView fx:id="todoListView" VBox.vgrow="ALWAYS" />
    </children>
</VBox>
