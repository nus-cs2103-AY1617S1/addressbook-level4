<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="Infinity" maxWidth="Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tars.ui.MainWindow">
   <children>
      <MenuBar minHeight="37.0" maxHeight="37.0" prefWidth="692.0" VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help" />
               </items>
            </Menu>
         </menus>
         <padding>
            <Insets bottom="5.0" left="15.0" top="5.0" />
         </padding>
      </MenuBar>
      <AnchorPane fx:id="infoHeaderPlaceholder" maxHeight="54.0" minHeight="50.0" prefHeight="54.0" prefWidth="692.0" styleClass="background" VBox.vgrow="NEVER" />
      <SplitPane id="splitPane" fx:id="splitPane" dividerPositions="0.5" VBox.vgrow="ALWAYS">
         <items>
            <VBox fx:id="taskList" prefHeight="0.0">
                <padding>
                    <Insets bottom="10.0" left="15.0" right="10.0" top="10.0" />
                </padding>
               <children>
                  <AnchorPane fx:id="taskListPanelPlaceholder" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <AnchorPane fx:id="browserPlaceholder">
                <padding>
                    <Insets bottom="10" left="5" right="10" top="10" />
                </padding>
            </AnchorPane>
         </items>
      </SplitPane>
       <AnchorPane fx:id="resultDisplayPlaceholder" maxHeight="100" minHeight="83.0" prefHeight="100.0" prefWidth="692.0" styleClass="anchor-pane" VBox.vgrow="NEVER">
           <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
           </padding>
       </AnchorPane>
      <AnchorPane fx:id="commandBoxPlaceholder" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <padding>
            <Insets bottom="0" left="0" right="0" top="0" />
         </padding>
      </AnchorPane>
      <AnchorPane fx:id="statusbarPlaceholder" VBox.vgrow="NEVER" />
   </children>
   <stylesheets>
   	  <URL value="@TarsTheme.css" />
      <URL value="@Extensions.css" />
   </stylesheets>
</VBox>
