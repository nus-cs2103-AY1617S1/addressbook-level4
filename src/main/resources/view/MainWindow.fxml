<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="547.0" prefWidth="824.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.address.ui.MainWindow">
   <children>
      <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" text="Close" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane fx:id="commandBoxPlaceholder" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </AnchorPane>
      <SplitPane id="splitPane" dividerPositions="0.5" VBox.vgrow="ALWAYS">
         <items>
            <VBox>
               <children>
                  <AnchorPane fx:id="resultDisplayPlaceholder" prefHeight="200.0" prefWidth="400.0" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
                     <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </padding>
                  </AnchorPane>
                  <SplitPane prefHeight="200.0" prefWidth="200.0" styleClass="anchor-pane-with-border" VBox.vgrow="ALWAYS">
                     <items>
                        <VBox fx:id="taskList" prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <AnchorPane fx:id="taskListLeftPanelPlaceholder" prefHeight="300.0" prefWidth="400.0" styleClass="anchor-pane" VBox.vgrow="ALWAYS" />
                           </children>
                           <padding>
                              <Insets left="10.0" right="10.0" top="5.0" />
                           </padding>
                        </VBox>
                     </items>
                     <stylesheets>
                        <URL value="@DarkTheme.css" />
                        <URL value="@Extensions.css" />
                     </stylesheets>
                  </SplitPane>
               </children>
            </VBox>
            <TabPane fx:id="tabPane" onKeyPressed="#handleQuickView" tabClosingPolicy="UNAVAILABLE" tabMaxHeight="15.0" tabMaxWidth="70.0">
               <tabs>
                  <Tab text="Today">
                     <content>
                        <SplitPane>
                           <items>
                              <VBox fx:id="todayTaskList">
                                 <children>
                                    <AnchorPane fx:id="todayTaskListTabPanelPlaceholder" styleClass="anchor-pane-with-border" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </items>
                        </SplitPane>
                     </content>
                  </Tab>
                  <Tab text="Tomorrow">
                     <content>
                        <SplitPane>
                           <items>
                              <VBox fx:id="tomorrowTaskList">
                                 <children>
                                    <AnchorPane fx:id="tomorrowTaskListTabPanelPlaceholder" styleClass="anchor-pane-with-border" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </items>
                        </SplitPane>
                     </content>
                  </Tab>
                  <Tab text="In 7 Days">
                     <content>
                        <SplitPane>
                           <items>
                              <VBox fx:id="in7DaysTaskList">
                                 <children>
                                    <AnchorPane fx:id="in7DaysTaskListTabPanelPlaceholder" styleClass="anchor-pane-with-border" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </items>
                        </SplitPane>
                     </content>
                  </Tab>
                  <Tab text="In 30 Days">
                     <content>
                        <SplitPane>
                           <items>
                              <VBox fx:id="in30DaysTaskList">
                                 <children>
                                    <AnchorPane fx:id="in30DaysTaskListTabPanelPlaceholder" styleClass="anchor-pane-with-border" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </items>
                        </SplitPane>
                     </content>
                  </Tab>
                  <Tab text="Someday">
                     <content>
                        <SplitPane>
                           <items>
                              <VBox fx:id="somedayTaskList">
                                 <children>
                                    <AnchorPane fx:id="somedayTaskListTabPanelPlaceholder" styleClass="anchor-pane-with-border" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </items>
                        </SplitPane>
                     </content>
                  </Tab>
               </tabs>
            </TabPane>
         </items>
      </SplitPane>
      <AnchorPane fx:id="statusbarPlaceholder" VBox.vgrow="NEVER" />
   </children>
   <stylesheets>
      <URL value="@DarkTheme.css" />
      <URL value="@Extensions.css" />
   </stylesheets>
</VBox>
